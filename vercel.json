{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "/2025-06-03/(.*)",
			"destination": "/2025-06-03/index.html"
		},
		{
			"source": "/2025-10-18/(.*)",
			"destination": "/2025-10-18/index.html"
		}
	],
	"redirects": [
		{
			"destination": "https://maditya.sh/talks",
			"source": "/",
			"permanent": true
		}
	],
	"buildCommand": "pnpm run --filter='*' build",
	"outputDirectory": "dist"
}
